<?xml version="1.0" encoding="UTF-8"?>
<config>
    <logger>
        <file value="/var/log/xparser.log" />
        <stream value="STDOUT" />
        <prefix value="it's prefix : " />
        <postfix value=" : it's postfix" />
        <timings value="true" />
    </logger>
	<parser>
        <root_dir value="/tmp/to_replace/" />
	</parser>
    <replacer>
        <stream_count value="8" />
        <thread_start_waiting value="100" />
        <pair>
            <from value="@value1@" />
            <to value="data1" />
        </pair>
        <pair>
            <from value="@value2@" />
            <to value="data2" />
        </pair>
        <pair>
            <from value="@value3@" />
            <to value="data3" />
        </pair>
    </replacer>
</config>
